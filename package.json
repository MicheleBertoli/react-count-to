{
  "name": "react-count-to",
  "version": "0.4.2",
  "description": "Animated counter component for React.js",
  "main": "dist/react-count-to.js",
  "scripts": {
    "build": "babel ./src --out-dir ./dist",
    "pretest": "eslint ./src/*",
    "test": "jest",
    "demo": "browserify ./demo/index.js -o ./demo/build.js",
    "start": "node server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MicheleBertoli/react-count-to.git"
  },
  "keywords": [
    "React.js",
    "react-component"
  ],
  "author": "Michele Bertoli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MicheleBertoli/react-count-to/issues"
  },
  "homepage": "https://github.com/MicheleBertoli/react-count-to",
  "devDependencies": {
    "babel": "^5.1.11",
    "babel-eslint": "^4.1.3",
    "babel-jest": "^12.0.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^6.0.2",
    "browserify": "^9.0.8",
    "eslint": "^1.6.0",
    "eslint-plugin-react": "^3.5.1",
    "express": "^4.12.3",
    "jest-cli": "^12.0.2",
    "react": "^15.0.2",
    "react-addons-test-utils": "^15.0.2",
    "react-dom": "^15.0.2",
    "superagent": "^1.2.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "jest": {
    "testPathDirs": [
      "./src"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/"
    ]
  },
  "engines": {
    "node": "0.10.33"
  }
}
