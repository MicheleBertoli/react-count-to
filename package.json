{
  "name": "react-count-to",
  "version": "0.12.0",
  "description": "Animated counter component for React.js",
  "main": "dist/react-count-to.js",
  "scripts": {
    "build": "babel ./src --out-dir ./dist --ignore __tests__",
    "lint": "eslint ./src/*",
    "pretest": "npm run lint",
    "test": "jest",
    "demo": "browserify ./demo/public/index.js -o ./demo/public/build.js",
    "start": "node ./demo/server",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MicheleBertoli/react-count-to.git"
  },
  "keywords": [
    "React.js",
    "react-component"
  ],
  "author": "Michele Bertoli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MicheleBertoli/react-count-to/issues"
  },
  "homepage": "https://github.com/MicheleBertoli/react-count-to",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-jest": "^23.6.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.13.4",
    "jest-cli": "^23.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "superagent": "^3.8.3"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "jest": {
    "roots": [
      "./src"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/prop-types/"
    ],
    "setupFiles": [
      "<rootDir>/src/__tests__/setup.js"
    ],
    "testMatch": [
      "**/?(*)+(spec|test).js"
    ]
  },
  "dependencies": {
    "prop-types": "^15.5.8"
  }
}
